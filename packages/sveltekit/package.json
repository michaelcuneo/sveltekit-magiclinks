{
	"name": "@sveltekit-sst-auth/frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "sst bind vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.16",
		"@smui/form-field": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.16",
		"@smui/textfield": "^7.0.0-beta.16",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/nodemailer": "^6.4.14",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"jsonwebtoken": "^9.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sanitize.css": "^13.0.0",
		"smui-theme": "^7.0.0-beta.16",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-kit-sst": "^2.40.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"uuid": "^9.0.1",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"electrodb": "^2.13.1",
		"nodemailer": "^6.9.11",
		"ulid": "^2.3.0"
	}
}
